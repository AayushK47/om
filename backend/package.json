{
  "name": "food-order-backend",
  "version": "1.0.0",
  "description": "Backend for food order management app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "vitest src/test/integration/ src/test/validation/",
    "test:run": "vitest run src/test/integration/ src/test/validation/",
    "test:ui": "vitest --ui src/test/integration/ src/test/validation/",
    "test:coverage": "vitest run --coverage src/test/integration/ src/test/validation/"
  },
  "keywords": [
    "express",
    "typescript",
    "sqlite",
    "food-orders"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.9.0",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
